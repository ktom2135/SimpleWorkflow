<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.tom.model">
	<class name="com.tom.model.AttendType" table="type_table">
		<id column="type_id" name="id" type="integer">
			<generator class="native" />
		</id>
		<property column="type_name" generated="never" lazy="false"
			name="name" not-null="true" type="string" />
		<property column="amerce_amount" generated="never" lazy="false"
			name="amerce" not-null="true" type="double" />
	</class>
</hibernate-mapping>
