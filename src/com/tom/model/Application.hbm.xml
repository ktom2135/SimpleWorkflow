<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE hibernate-mapping
	PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.tom.model">
	<class name="Application" table="app_table">
		<id name="id" type="integer" column="app_id">
			<generator class="identity"></generator>
		</id>
		<property name="reason" column="app_reason" type="string"
			length="500"></property>
		<property name="result" column="app_result" type="boolean"></property>

		<many-to-one name="type" column="type_id" class="AttendType"
			not-null="true" lazy="false"></many-to-one>
		<many-to-one name="attend" column="attend_id" class="Attend"
			not-null="true" lazy="false"></many-to-one>
		<one-to-one name="check" property-ref="app"></one-to-one>
	</class>
</hibernate-mapping>