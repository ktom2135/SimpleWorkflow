<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE hibernate-mapping
	PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.tom.model">
	<class name="Attend" table="attend_table">
		<id name="id" type="integer" column="attend_id">
			<generator class="identiry"></generator>
		</id>
		<property name="dutyDay" column="duty_day" type="string"
			not-null="true" length="50"></property>
		<property name="punchTime" column="punch_time" type="java.util.Date"></property>

		<property name="isCome" column="is_Come" type="boolean"
			not-null="true"></property>
		<many-to-one name="type" column="type_id" class="AttendType"
			not-null="true" lazy="false"></many-to-one>

		<many-to-one name="employee" column="emp_id" class="Employee"
			not-null="true" lazy="false"></many-to-one>
	</class>

</hibernate-mapping>
	
